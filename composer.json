{
    "name": "willypuzzle/vuetify-php",
    "description": "A backend Vuetify package.",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/willypuzzle/vuetify-php/issues",
        "source": "https://github.com/willypuzzle/vuetify-php"
    },
    "authors": [
        {
            "name": "Domenico Rizzo",
            "email": "domenico.rizzo@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/support": ">=5.4.0",
        "illuminate/database": ">=5.4.0",
        "league/fractal": "^0.16.0"
    },
    "autoload": {
        "psr-4": {
            "Idsign\\Vuetify\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Idsign\\Vuetify\\VuetifyServiceProvider"
            ]
        }
    }
}
